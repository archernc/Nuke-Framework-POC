<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Develop</name>
  <description />
  <settings>
    <parameters>
      <param name="env.Git_Branch" value="%teamcity.build.vcs.branch.Poc_NukeFramework_HttpsIntegrationGitBirchComScmTpNukeCoreTestGit%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="Nuke Build" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_scriptArguments" value="--nologo --target Test" />
          <param name="jetbrains_powershell_script_file" value="build.ps1" />
          <param name="jetbrains_powershell_script_mode" value="FILE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Poc_NukeFramework_HttpsIntegrationGitBirchComScmTpNukeCoreTestGit" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:refs/heads/develop" />
          <param name="groupCheckinsByCommitter" value="true" />
          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
          <param name="triggerRules" value="-:comment=(\*{3}NO_CI\*{3}):**" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

